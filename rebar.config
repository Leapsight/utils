{minimum_otp_vsn, "R23"}.
{erl_opts, [debug_info]}.
{deps, [
    {ksuid,
        {git, "https://github.com/leapsight/ksuid.git", {branch, "master"}}
    },
    {app_config,
        {git, "https://github.com/leapsight/app_config.git", {tag, "1.1.1"}}
    },
    {key_value,
        {git, "https://github.com/Leapsight/key_value.git", {tag, "1.1.1"}}
    },
    {maps_utils,
        {git, "https://github.com/Leapsight/maps_utils.git", {tag, "1.0.0"}}
    }
]}.

{project_plugins, [rebar3_proper]}.

{profiles, [
    {test, [
        {erl_opts, [nowarn_export_all]},
        {deps, [proper]}
    ]}
]}.

{cover_enabled, true}.
